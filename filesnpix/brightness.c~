int main(  char* argv[] ) {
        FILE *fp;
        fp = fopen( argv[1], "rb" );
        
       /* printf(" %i ", argc );
*/
        /*
        Pnmrdr_T test; 
        */

        if( !fclose( fp ) ) { 
                exit ( EXIT_FAILURE ); 
        }

